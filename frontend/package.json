{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "final-form": "^4.19.1",
    "final-form-material-ui": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.4.0",
    "react-loader-spinner": "^3.1.14",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "3.4.1",
    "recompose": "^0.30.0",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "start": "sh  -ac 'cp .env.${REACT_APP_ENV} .env; react-scripts start'",
    "start:dev": "npm run start:dev:staging-api",
    "start:dev:staging-api": "REACT_APP_ENV=staging npm run start",
    "start:dev:production-api": "REACT_APP_ENV=production npm run start",
    "build": "sh  -ac 'cp .env.${REACT_APP_ENV} .env; react-scripts build'",
    "build:production": "REACT_APP_ENV=production npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "node-sass": "^4.14.1"
  }
}
