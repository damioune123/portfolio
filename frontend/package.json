{
  "name": "nextjs-react-redux-filter",
  "version": "1.0.0",
  "main": "server/index.js",
  "repository": "https://github.com/yrfang/nextjs-react-redux-filter.git",
  "author": "Ying Ru <fyr1116@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  nodemon server/index.js'",
    "dev:staging": "REACT_APP_ENV=staging npm run dev",
    "dev:prod": "REACT_APP_ENV=prod npm run dev",
    "dev:local": "REACT_APP_ENV=local npm run dev",
    "export": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  next export'",
    "export:staging": "REACT_APP_ENV=staging npm run export",
    "export:prod": "REACT_APP_ENV=prod npm run export",
    "export:local": "REACT_APP_ENV=local npm run export",
    "build": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  next build'",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "start": "sh  -ac 'cp .env.${REACT_APP_ENV} .env;  node server/index.js'",
    "start:staging": "REACT_APP_ENV=staging npm run start",
    "start:prod": "REACT_APP_ENV=prod npm run start",
    "start:local": "REACT_APP_ENV=local npm run start"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "@directus/sdk-js": "^6.1.1",
    "@zeit/next-sass": "^0.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "next": "^6.0.3",
    "next-redux-saga": "^3.0.0-beta.1",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "next-routes": "^1.4.2",
    "node-sass": "^4.9.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-lazy-load": "^3.0.13",
    "react-loader-spinner": "^3.1.14",
    "react-redux": "^5.0.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "nodemon": "^1.17.5",
    "postcss-easy-import": "^3.0.0"
  }
}
